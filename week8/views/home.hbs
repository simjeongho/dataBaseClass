<h1>Employee 데이터 삽입</h1>
<form name="employee" method="post" action="/">
  <div>
    <label for="fname">Fname</label>
    <input id="fname" name="fname" type="text" required placeholder="Fname" />
  </div>
  <div>
    <label for="minit">Minit</label>
    <input id="minit" name="minit" type="text" required placeholder="Minit" />
  </div>
  <div>
    <label for="lname">lname</label>
    <input id="lname" name="lname" type="text" required placeholder="Lname" />
  </div>
  <div>
    <label for="ssn">ssn</label>
    <input id="ssn" name="ssn" type="text" required placeholder="Ssn" />
  </div>
  <div>
    <label for="bdate">bdate</label>
    <input id="bdate" name="bdate" type="text" required placeholder="Bdate" />
  </div>
  <div>
    <label for="address">address</label>
    <input
      id="address"
      name="address"
      type="text"
      required
      placeholder="Address"
    />
  </div>
  <div>
    <label for="sex">sex</label>
    <input id="sex" name="sex" type="text" required placeholder="Sex" />
  </div>
  <div>
    <label for="salary">salary</label>
    <input
      id="salary"
      name="salary"
      type="text"
      required
      placeholder="Salary"
    />
  </div>
  <div>
    <label for="super_ssn">super_ssn</label>
    <input
      id="super_ssn"
      name="super_ssn"
      type="text"
      placeholder="Super_ssn"
    />
  </div>
  <div>
    <label for="dno">dno</label>
    <input id="dno" name="dno" type="text" required placeholder="Dno" />
  </div>
  <div>
    <input type="submit" value="삽입" />
  </div>

</form>
<br /><br />
