<h1>{{title}}</h1>
<table>
  <tr>
    <td>Dname</td>
    <td>Dnumber</td>
  </tr>
  {{#each nonIt}}
    <form method="post">
      <tr>
        <td>{{Dname}}</td>
        <td>{{Dnumber}}</td>
        <td style="border: none; margin-left: 10px;">
          <button
            style="margin-left: 10px;"
            name="delBtn"
            type="submit"
            value="{{Dname}}"
            formaction="/deleteNonIt"
          >
            삭제
          </button>
        </td>
      </tr>
    </form>
  {{/each}}
</table>